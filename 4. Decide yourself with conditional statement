import Swift

/**
  * Read input from STDIN. Print your output to STDOUT 
  * Use readLine() to read input from STDIN
  * use print to write your output to STDOUT
*/
// Below is an example on how to take integer input and print it.
// a = parse(Int,readline(STDIN))
// print(a)

let age: Int = Int(readLine()!) ?? 0
if age < 10{
print("I am happy as having no responsibilities.")
}
else if age >= 10 && age < 18 {
print("I am still happy but starts feeling pressure of life.")
}
else if age >= 18{
    print("I am very much happy as i handled the pressure very well.")
}
